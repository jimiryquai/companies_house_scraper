# Companies House API Keys (3 Separate Developer Hub Applications)
# Application #1: Streaming API
CH_STREAMING_API_KEY=your_streaming_api_key_here

# Application #2: Companies REST API (600 requests / 5 minutes)
CH_COMPANIES_API_KEY=your_companies_api_key_here

# Application #3: Officers REST API (600 requests / 5 minutes)
CH_OFFICERS_API_KEY=your_officers_api_key_here

# PostgreSQL Database
# Format: postgresql://username:password@host:port/database
# Railway provides this automatically as DATABASE_URL
DATABASE_URL=postgresql://user:password@localhost:5432/companies_house

# Logging
LOG_LEVEL=INFO
